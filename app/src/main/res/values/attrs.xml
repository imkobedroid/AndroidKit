<?xml version="1.0" encoding="utf-8"?>
<resources>

  <attr format="string" name="title"/>
  <attr format="color" name="color"/>
  <attr format="dimension" name="size"/>
  <attr name="image" format="reference"/>
  <attr name="imageScaleType">
